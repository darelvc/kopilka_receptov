<div class="widget wow fadeInRight" data-wow-delay=".2s">
	<h3>Tips and tricks</h3>
	<ul class="articles_latest">
		<% @most_liked.each do |recipe| %>
			<li>
				<%= link_to recipe_path(recipe) do %>
					<%= image_tag("img9.jpg") %>
					<h6><%= raw recipe.summary.truncate(25) %></h6>
				<% end %>
			</li>
		<% end %>
	</ul>
</div>